<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
 	
 	<bean id="mathCheatObject" class="com.springproject.diloosecoupling.MathCheat"></bean>
 	
 	<bean id="scienceCheatObject" class="com.springproject.diloosecoupling.ScienceCheat"></bean>
 	
 	<bean id="student" class="com.springproject.diloosecoupling.Student">
 		<property name="cheat" ref="mathCheatObject" />
 	</bean>
 	 
 </beans>