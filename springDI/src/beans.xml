<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
   
   <bean id="student" class="com.springproject.di.Student">
		
		<property name="name" value="Siam" />
		<property name="id" value="123" />
		   		
   </bean>
   
   <bean id="teacher" class="com.springproject.di.Teacher">
		
		<constructor-arg name="name" value="Faiz" />
		<constructor-arg name="id" value="232" type="int" />
		   		
   </bean>
   
</beans>